{
  "name": "i18n",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-ivy": "ng build --configuration=production,fr-ivy",
    "serve-ivy": "serve dist/i18n/ivy/fr --single --listen 4200",
    "build-compat": "ng build --configuration=production,fr-compat",
    "serve-compat": "serve dist/i18n/compat --single --listen 4200",
    "build-ve": "ng build --configuration=production,fr-compat,ve",
    "serve-ve": "serve dist/i18n/ve --single --listen 4200",
    "test": "ng test",
    "lint": "ng lint",
    "e2e-ivy": "npm-run-all -p -r \"serve:e2e dist/i18n/ivy/fr \" protractor",
    "e2e-compat": "npm-run-all -p -r \"serve:e2e dist/i18n/compat \" protractor",
    "e2e-ve": "npm-run-all -p -r \"serve:e2e dist/i18n/ve \" protractor",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "serve:e2e": "serve --single --listen 4200",
    "protractor": "ng e2e --dev-server-target="
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0-next.14",
    "@angular/common": "9.0.0-next.14",
    "@angular/compiler": "9.0.0-next.14",
    "@angular/core": "9.0.0-next.14",
    "@angular/forms": "9.0.0-next.14",
    "@angular/localize": "^9.0.0-next.11",
    "@angular/platform-browser": "9.0.0-next.14",
    "@angular/platform-browser-dynamic": "9.0.0-next.14",
    "@angular/router": "9.0.0-next.14",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.0-next.18",
    "@angular/cli": "9.0.0-next.18",
    "@angular/compiler-cli": "9.0.0-next.14",
    "@angular/language-service": "9.0.0-next.14",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.14.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.2",
    "serve": "^11.2.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "3.6.4"
  }
}
